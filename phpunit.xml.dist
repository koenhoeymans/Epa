<phpunit>
	<testsuites>
		<testsuite name="unittests">
			<directory>tests/unit</directory>
        </testsuite>
        <testsuite name="end2endtests">
            <directory>tests/e2e</directory>
        </testsuite>
	</testsuites>
    <filter>
        <blacklist>
            <directory>./vendor</directory>
        </blacklist>
    </filter>
    <logging>
        <log type="coverage-html" target="./build/codecoverage" />
    </logging>
</phpunit>
